server {
  if ($host = www.ministry.b4e.vn) {
    return 301 https://$host$request_uri;
  } 
  if ($host = ministry.b4e.vn) {
    return 301 https://$host$request_uri;
  } 
  listen 80;
  server_name ministry.b4e.vn www.ministry.b4e.vn;
    return 404; 
}

