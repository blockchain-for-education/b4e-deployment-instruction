server {
    listen 443; 
    server_name explorer.b4e.vn www.explorer.b4e.vn;

    location / {
      try_files $uri $uri/ index.html =404;
    }

    ssl_certificate /etc/letsencrypt/live/explorer.b4e.vn/fullchain.pem; 
